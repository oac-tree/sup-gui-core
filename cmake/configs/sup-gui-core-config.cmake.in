#  SUP_GUI_CORE_INCLUDE_DIRS - include directories
#  SUP_GUI_CORE_LIBRARIES    - libraries to link against

get_filename_component(SUP_GUI_CORE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

include(CMakeFindDependencyMacro)

find_dependency(sup-dto)
find_dependency(sup-mvvm)
find_dependency(sup-gui-extra)

if(TARGET sup-gui-core::core OR TARGET sup-gui-core::tools)
else()
  message(STATUS "sup-gui-core-config.cmake including ${SUP_GUI_CORE_CMAKE_DIR}/sup-gui-core-targets.cmake")
  include("${SUP_GUI_CORE_CMAKE_DIR}/sup-gui-core-targets.cmake")
endif()

set(SUP_GUI_CORE_LIBRARIES sup-gui-core::core sup-gui-core::tools)

message(STATUS "sup-gui-core-config.cmake in ${CMAKE_CURRENT_LIST_FILE}")
message(STATUS "SUP_GUI_CORE_LIBRARIES ${SUP_GUI_CORE_LIBRARIES}")
